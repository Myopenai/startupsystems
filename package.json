{
  "name": "startupsystems",
  "version": "1.0.0",
  "description": "Modern startup system infrastructure with Cloudflare Workers",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "npm run build:html && wrangler deploy",
    "test": "vitest",
    "build": "npm run build:html && npm run build:workers && npm run build:assets",
    "build:html": "node scripts/build-html-templates.js",
    "build:workers": "wrangler build",
    "build:assets": "echo 'Build assets'",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "startup",
    "cloudflare",
    "workers",
    "serverless",
    "edge-computing"
  ],
  "author": "Myopenai",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@types/node": "^20.10.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "wrangler": "^4.50.0"
  },
  "dependencies": {
    "hono": "^3.12.0"
  }
}
